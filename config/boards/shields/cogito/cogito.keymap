combos {
        compatible = "zmk,combos";
        combo_esc {
                timeout-ms = <TIMEOUT>;
                key-positions = <0 1>;
                bindings = <&kp ESC>;
            };

        combo_win {
                timeout-ms = <TIMEOUT>;
                key-positions = <30 31>;
                bindings = <&kp LGUI>;
            };


        combo_game {
                timeout-ms = <TIMEOUT>;
                key-positions = <30 31 32>;
                bindings = <&tog GAME>;
            };
